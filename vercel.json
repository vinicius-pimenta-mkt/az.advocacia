{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "dest": "public/index.html"
    }
  ],
  "env": {
    "JWT_SECRET": "35b75ccd96165e47c2308a85d821bb4daa84c91efbd282895f176db2eeb945a7",
    "NODE_ENV": "production"
  }
}
